<?xml version="1.0"?>

<PropertyList>
    <name>propulsion</name>
    <dialog-name>propulsion</dialog-name>
    <x>-10</x>
    <y>-30</y>
    <layout>vbox</layout>
	    <group>
		<layout>hbox</layout>
		<default-padding>1</default-padding>
		<empty>
		<stretch>true</stretch>
		</empty>
		<text>
		    <label>Propulsion Mode</label>
		</text>
		<empty>
		<stretch>true</stretch>
		</empty>
		<button>
		<legend/>
		<key>Esc</key>
		<pref-width>16</pref-width>
		<pref-height>16</pref-height>
		<border>2</border>
		<command>dialog-apply</command>
                <binding>
		    <command>dialog-close</command>
                </binding>
		</button>
	    </group>
	    <hrule/>
	    
	    <group>
                <layout>table</layout>
		<text>
                    <label>MIXTURE very rich</label>
                    <halign>left</halign>
		    <row>0</row>
		     <col>0</col>
                </text>
		<radio>
		    <row>0</row>
                    <col>1</col>
		    <property>sim/model/engine-mixture-rich</property>
		    <live>true</live>
		     <binding>
			<command>property-assign</command>
			<property>sim/model/engine-mixture-rich</property>
			<value>1</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>sim/model/engine-mixture-lean</property>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>/controls/engines/engine[0]/mixture</property>
			<value>1</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>/controls/engines/engine[1]/mixture</property>
			<value>1</value>
		    </binding>
		</radio>
		<text>
                    <label>MIXTURE lean</label>
                    <halign>left</halign>
		    <row>1</row>
		     <col>0</col>
                </text>
		<radio>
		    <row>1</row>
                    <col>1</col>
		    <property>sim/model/engine-mixture-lean</property>
		    <live>true</live>
		     <binding>
			<command>property-assign</command>
			<property>sim/model/engine-mixture-rich</property>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>sim/model/engine-mixture-lean</property>
			<value>1</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>/controls/engines/engine[0]/mixture</property>
			<value>0.99</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>/controls/engines/engine[1]/mixture</property>
			<value>0.99</value>
		    </binding>
		</radio>
		<text>
		    <label>TakeOff Power</label>
		    <halign>left</halign>
		    <row>2</row>
                    <col>0</col>
		</text>
                <radio>
		    <row>2</row>
                    <col>1</col>
		    <property>fdm/jsbsim/systems/boost-cmd/takeoff</property>
		    <live>true</live>
		    <binding>
			 <command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/takeoff</property>
			<command>property-assign</command>
			<value>1</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/cruise</property>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/climb</property>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/manual</property>
			<value>0</value>
		    </binding>
		</radio>
		<text>
		    <label>Climb Power</label>
		    <halign>left</halign>
		    <row>3</row>
                    <col>0</col>
		</text>
		<radio>
		    <row>3</row>
                    <col>1</col>
		    <property>fdm/jsbsim/systems/boost-cmd/climb</property>
		    <live>true</live>
		    <binding>
			 <command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/takeoff</property>
			<command>property-assign</command>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/cruise</property>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/climb</property>
			<value>1</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/manual</property>
			<value>0</value>
		    </binding>
		</radio>
		<text>
		    <label>Cruise Power</label>
		    <halign>left</halign>
		    <row>4</row>
                    <col>0</col>
		</text>
		<radio>
		    <row>4</row>
                    <col>1</col>
		    <property>fdm/jsbsim/systems/boost-cmd/cruise</property>
		    <live>true</live>
		    <binding>
			 <command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/takeoff</property>
			<command>property-assign</command>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/cruise</property>
			<value>1</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/climb</property>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/manual</property>
			<value>0</value>
		    </binding>
		</radio>
		<text>
		    <label>Manual</label>
		    <halign>left</halign>
		    <row>5</row>
                    <col>0</col>
		</text>
		<radio>
		    <row>5</row>
                    <col>1</col>
		    <property>fdm/jsbsim/systems/boost-cmd/manual</property>
		    <live>true</live>
		    <binding>
			 <command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/takeoff</property>
			<command>property-assign</command>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/cruise</property>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/climb</property>
			<value>0</value>
		    </binding>
		    <binding>
			<command>property-assign</command>
			<property>fdm/jsbsim/systems/boost-cmd/manual</property>
			<value>1</value>
		    </binding>
		</radio>		
		
            </group>
	   
	   
	    

</PropertyList>
