<!-- Junkers Jumo 205 -->

<piston_engine name="Junkers Jumo 205">

	<minmp unit="INHG">         10.0 </minmp>
	<maxmp unit="INHG">         48.5 </maxmp>
	<displacement unit="IN3"> 1015.00 </displacement>
	<maxhp>        880.00 </maxhp>
